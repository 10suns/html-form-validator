{
    "name": "xtreamwayz/html-form-validator",
    "type": "project",
    "homepage": "https://github.com/xtreamwayz/html-form-validator",
    "license": "MIT",
    "authors": [
        {
            "name": "Geert Eltink",
            "homepage": "https://xtreamwayz.github.io/"
        }
    ],
    "require": {
        "zendframework/zend-validator": "^2.5",
        "zendframework/zend-filter": "^2.5",
        "zendframework/zend-inputfilter": "^2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "scripts": {
        "post-install-cmd": [
            "rm -f data/cache/app_config.*",
            "rm -rf data/cache/stash/",
            "rm -rf data/cache/twig/"
        ],
        "post-update-cmd": [
            "rm -f data/cache/app_config.*",
            "rm -rf data/cache/stash/",
            "rm -rf data/cache/twig/"
        ],
        "check": [
            "@cs",
            "@test"
        ],
        "cs": "php vendor/squizlabs/php_codesniffer/scripts/phpcs",
        "test": "php vendor/phpunit/phpunit/phpunit",
        "serve": "php -S 0.0.0.0:8000 -t public/"
    },
    "autoload": {
        "psr-4": {
            "Xtreamwayz\\HTMLFormValidator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Xtreamwayz\\HTMLFormValidatorTest\\": "test/"
        }
    }
}
