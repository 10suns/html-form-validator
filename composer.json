{
    "name": "xtreamwayz/html-form-validator",
    "type": "project",
    "homepage": "https://github.com/xtreamwayz/html-form-validator",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/xtreamwayz/html-form-validator/issues",
        "wiki": "https://github.com/xtreamwayz/html-form-validator/wiki"
    },
    "authors": [
        {
            "name": "Geert Eltink",
            "homepage": "https://xtreamwayz.github.io/"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/weierophinney/zend-validator",
            "reference": "sm-v2-v3-compat"
        },
        {
            "type": "vcs",
            "url": "https://github.com/kynx/zend-inputfilter",
            "reference": "feature/50"
        }
    ],
    "require": {
        "php": "^7.0",
        "zendframework/zend-inputfilter": "dev-sm-v2-v3-compat as 2.6.0",
        "zendframework/zend-validator": "dev-feature/50 as 2.6.0",
        "zendframework/zend-filter": "^2.6",
        "zendframework/zend-servicemanager": "^3.0",
        "zendframework/zend-i18n": "^2.6",
        "zendframework/zend-uri": "^2.5",
        "zendframework/zend-stdlib": "dev-develop as 2.8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0",
        "codeception/aspect-mock": "*",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "scripts": {
        "check": [
            "@cs",
            "@test"
        ],
        "cs": "php vendor/squizlabs/php_codesniffer/scripts/phpcs --standard=./build/phpunit.xml.dist",
        "test": "php vendor/phpunit/phpunit/phpunit --configuration ./build/phpunit.xml.dist"
    },
    "autoload": {
        "psr-4": {
            "Xtreamwayz\\HTMLFormValidator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "XtreamwayzTest\\HTMLFormValidator\\": "test/"
        }
    }
}
