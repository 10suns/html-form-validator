--TEST--
Test data filters attribute with reusing submitted values
--HTML-FORM--
<form action="/" method="post">
    <input type="text" id="name" name="name"
        data-reuse-submitted-value="true"
        data-filters="striptags|stringtrim"
    />
</form>
--DEFAULT-VALUES--
{
    "name": "default value"
}
--SUBMITTED-VALUES--
{
    "name": "  Full Name  "
}
--EXPECTED-VALUES--
{
    "name": "Full Name"
}
--EXPECTED-FORM--
<form action="/" method="post">
    <input type="text" id="name" name="name"
           data-reuse-submitted-value="true" value="Full Name"
    />
</form>
--EXPECTED-ERRORS--
{
}
